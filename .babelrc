{
	"presets": [
		["env", {
			"targets": {
				"browsers": ["last 2 versions"]
			},
			"modules": false,
			"useBuiltIns": true,
			"debug": false
		}],
		"react"
	],
	"plugins": [
		"transform-react-jsx",
		"transform-object-rest-spread",
		"transform-decorators",
		"transform-class-properties",
		"syntax-dynamic-import",
		["transform-runtime", {
			"helpers": true,
			"polyfill": false,
			"regenerator": true,
			"moduleName": "babel-runtime"
		}]
    ],
	"ignore": ["*.ignore.js"]
}
